// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Categories {
  id           Int    @id @default(autoincrement())
  category1    String
  category2    String
  category3    String
  categoryId   String
  categoryName String
}

model Recipes {
  id                      Int      @id @default(autoincrement())
  recipeTitle             String
  recipeUrl               String
  recipeDescription       String
  foodImageUrls           String[]
  keywords                String[]
  totalTime               Int
  recipeMaterial          String[]
  recipeMaterialConverted String?
}

model RecipesTmp {
  id                Int      @id @default(autoincrement())
  recipeTitle       String
  recipeUrl         String
  recipeDescription String
  foodImageUrls     String[]
  keywords          String[]
  totalTime         Int
  recipeMaterial    String[]
}

model Urls {
  id Int @id @default(autoincrement())
  url String
  structured Boolean
}
